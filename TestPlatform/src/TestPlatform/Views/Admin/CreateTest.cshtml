@model TestSettingsFormVM
@{
    Layout = "_SmallInputLayout";
}
<form asp-controller="Admin" asp-action="CreateTest">
    <h1>Create Test</h1>
    <label asp-for="TestName"></label>
    <input asp-for="TestName" class="form-control" />
    <span asp-validation-for="TestName"></span>
    <br />
    <label asp-for="Description"></label>
    <input asp-for="Description" class="form-control" />
    <span asp-validation-for="Description"></span>
    <br />
    <label asp-for="Tags"></label>
    <input asp-for="Tags" class="form-control" />
    <span asp-validation-for="Tags"></span>
    <button onclick="window.history.back()" class="orcaBtn btnWidth120 TopMargin20 bgRed"><span class="fa fa-angle-left rightMargin5" aria-hidden="true"></span>Go Back</button>
    <button type="submit" class="orcaBtn TopMargin20 onTheLine"><span class="fa fa-check rightMargin5"></span>Create Test</button>
    @*<input type="submit" value="Create Test" class="orcaBtn btnWidth120 TopMargin20 onTheLine" />*@
</form>

